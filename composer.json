{
    "name": "escsrl/user",
    "type": "library",
    "description": "Symfony base user configuration",
    "keywords": [
        "user",
        "symfony",
        "symfony-user"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Bellanda",
            "email": "a.bellanda@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Esc\\User\\": "src/"
        }
    },
    "autoload-dev": {
